        </preConditions>
        <rule name="Secure Request Verification Token">
          <match serverVariable="RESPONSE_Set_Cookie" pattern="__RequestVerificationToken=(.*)"/>
          <action type="Rewrite" value="{R:0};httponly"/>
        </rule>
      </outboundRules>
