      <rules>
        <rule name="Redirect Login.aspx to LV home" stopProcessing="true">
          <match url="^(login.aspx)/?$"/>
          <action type="Redirect" url="https://www.lv.com/insurance"/>
        </rule>
        <rule name="Redirect Insurance URL back to LV GI" stopProcessing="true">
          <match url="-/insurance/media/.*/sitecore_media.ashx/&quot;(/-/insurance/media/.*)"/>
          <action type="Redirect" url="https://www.lv.com/insurance"/>
        </rule>
        <rule name="Redirect Insurance URL back to LV GI with quotes" stopProcessing="true">
          <match url="-/insurance/media/.*/sitecore_media.ashx(/-/insurance/media/.*)"/>
          <action type="Redirect" url="https://www.lv.com/insurance"/>
        </rule>
        <rule name="Redirect Insurance URL back to LV-GI Single" stopProcessing="true">
          <match url="(/-/insurance/media/.*)"/>
          <action type="Redirect" url="https://www.lv.com/insurance"/>
        </rule>
        <rule name="Athena Error Page" stopProcessing="true">
          <match url=".*"/>
          <conditions>
            <add input="{HTTP_HOST}" pattern=".*lvadviser.com"/>
            <add input="{URL}" pattern="^/500.aspx$"/>
          </conditions>
          <action type="Rewrite" url="/Athena500.html"/>
        </rule>
        <rule name="Insurance Rewrite Asset Url">
          <match url="insurance/(dist/.*)"/>
          <action type="Rewrite" url="{R:1}"/>
        </rule>
        <rule name="Life Rewrite Asset Url">
          <match url="life/(dist/.*)"/>
          <action type="Rewrite" url="{R:1}"/>
        </rule>
        <rule name="Athena Rewrite Asset Url">
          <match url="life/(athenadist/.*)"/>
          <action type="Rewrite" url="{R:1}"/>
        </rule>
      </rules>
      <outboundRules>
        <rule name="Add RESPONSE_X-Frame-Options" preCondition="X-Frame-Option not present">
          <match serverVariable="RESPONSE_X_Frame_Options" pattern=".*"/>
          <action type="Rewrite" value="DENY"/>
        </rule>
        <preConditions>
          <preCondition name="X-Frame-Option not present">
